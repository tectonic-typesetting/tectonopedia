{
  "name": "Tectonopedia",
  "dockerFile": "Dockerfile",
  "forwardPorts": [
    1234
  ],
  "portsAttributes": {
    "1234": {
      "label": "Application",
      "onAutoForward": "openBrowserOnce"
    }
  },
  "overrideCommand": false,
  "postCreateCommand": "yarn install && cargo build --release --features=external-harfbuzz",
  "postStartCommand": "nohup bash -c 'cargo run --release --features=external-harfbuzz -- watch &'"
}